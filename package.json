{
  "name": "cakephp-calendar-app",
  "description": "CakePHP Calendar Plugin",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack --config ./resources/build/webpack.dev.conf.js --progress --hide-modules",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./resources/build/webpack.prod.conf.js --progress --hide-modules",
    "clean": "rimraf ./webroot/dist ./resources/test/unit/dist",
    "dev": "yarn build:dev && webpack-dev-server --config ./resources/build/webpack.dev.conf.js",
    "watch": "cross-env NODE_ENV=development webpack --config ./resources/build/webpack.hot.conf.js --progress --hide-modules --watch --watch-poll",
    "lint:js": "./node_modules/eslint/bin/eslint.js --config ./resources/.eslintrc.js --ext js --ext vue ./resources/src",
    "test:unit": "cross-env BABEL_ENV=test karma start ./resources/test/unit/karma.conf.js --single-run"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@jest/core": "^24.9.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "chokidar": "^3.3.0",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "dotenv": "^4.0.0",
    "eslint": "^6.6.0",
    "eslint-plugin-vue": "^5.2",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "jest-cli": "^24.9.0",
    "jest-haste-map": "^24.9.0",
    "jest-resolve": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "style-loader": "^0.17.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uppercamelcase": "^3.0.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "daterangepicker": "^2.1.27",
    "fullcalendar": "3.8.2",
    "inflected": "^2.0.4",
    "moment": "^2.20.1",
    "node-sass": "^4.13.0",
    "rrule": "^2.2.0",
    "sass-loader": "^7.3.1",
    "svg-url-loader": "^2.3.2",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.1",
    "vue-js-modal": "^1.3.31",
    "vue-notification": "^1.3.20",
    "vue-select": "^2.5.1",
    "vue-uuid": "^1.1.1",
    "vuejs-timepicker": "^0.1.4",
    "vuex": "^3.1.2"
  }
}

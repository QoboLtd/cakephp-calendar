{
  "name": "cakephp-calendar-app",
  "description": "CakePHP Calendar Plugin",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack --config ./resources/build/webpack.dev.conf.js --progress --hide-modules",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./resources/build/webpack.prod.conf.js --progress --hide-modules",
    "clean": "rimraf ./webroot/dist ./resources/test/unit/dist",
    "dev": "yarn build:dev && webpack-dev-server --config ./resources/build/webpack.dev.conf.js",
    "watch": "cross-env NODE_ENV=development webpack --config ./resources/build/webpack.hot.conf.js --progress --hide-modules --watch --watch-poll",
    "lint:js": "./node_modules/eslint/bin/eslint.js --config ./resources/.eslintrc.js --ext js --ext vue ./resources/src",
    "test:unit": "cross-env BABEL_ENV=test karma start ./resources/test/unit/karma.conf.js --single-run"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@jest/core": "^24.5.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "chokidar": "^3.0.1",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^2.1.1",
    "dotenv": "^4.0.0",
    "eslint": "^6.3.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.5",
    "karma": "^1.7.0",
    "karma-chai-dom": "^1.1.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-coverage": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.0",
    "mocha": "^3.5.0",
    "mocha-css": "^1.0.1",
    "nightwatch": "^0.9.16",
    "rimraf": "^2.6.0",
    "sinon": "2.2.0",
    "sinon-chai": "^2.10.0",
    "style-loader": "^0.17.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "uppercamelcase": "^3.0.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "daterangepicker": "^2.1.27",
    "fullcalendar": "3.8.2",
    "inflected": "^2.0.4",
    "moment": "^2.20.1",
    "node-sass": "^4.9.0",
    "rrule": "^2.2.9",
    "sass-loader": "^7.0.1",
    "svg-url-loader": "^2.3.2",
    "vue": "^2.6.10",
    "vue-grid-layout": "^2.3.4",
    "vue-js-modal": "^1.3.31",
    "vue-notification": "^1.3.16",

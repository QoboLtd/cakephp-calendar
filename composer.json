{
    "name": "qobo/cakephp-calendar",
    "description": "Calendaring plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Andrey Vystavkin",
            "email": "andrey.vystavkin@gmail.com"
        }
    ],
    "require": {
        "qobo/cakephp-utils": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit":"^5.0",
        "cakephp/cakephp-codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Qobo\\Calendar\\": "src/",
            "Qobo\\Calendar\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qobo\\Calendar\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
